{
    "manifest_version":3,
    "name": "myQuotesTranslateExtension",
    "version": "1.0",

    "description": "An extension to display a random famous quote and translate it!",
    "icons":{
        "48": "icon.png"
    },
  
    "action":{
      "default_title": "Click Me", 
      "default_popup": "popup.html"
    },

    "host_permissions":[
        "https://api.forismatic.com/*",
        "https://api.mymemory.translated.net/*"
    ]
}
